{
  "rewrites": [
    {
      "source": "/ache/(.*)",
      "destination": "/api/proxy"
    }
  ],
  "headers": [
    {
      "source": "/ache/(.*)",
      "headers": [
        {
          "key": "X-Real-IP",
          "value": "$remote_addr"
        },
        {
          "key": " X-Forwarded-For",
          "value": "$proxy_add_x_forwarded_for"
        },
        {
          "key": "Host",
          "value": "$http_host"
        }
      ]
    }
  ]
}
